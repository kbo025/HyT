        <div class='col-md-6 col-xs-16'>
            <div class='nvc-form-group'>
                <label for='cuenta bancaria'>Cuenta bancaria</label>
                <input
                    class='nvc-input__register'
                    type='text'
                    name='bank_account'
                    ng-focus="formStep3.bank_account.$setUntouched()"
                    ng-model='registrationDataStep3.bank_account'
                    minLength="20"
                    maxlength="20"
                    pattern="(\d){20}"/>
            </div>
             <span
                ng-cloak
                class="error"
                ng-show="formStep3.bank_account.$error.pattern &&
                formStep3.bank_account.$touched">
                {{ "share.validation.error_bank_account"|trans }}
            </span>
            <span
                ng-cloak
                class="error"
                ng-show="validations && registrationDataStep3.bank_account == null">
                {{ "aavv.register.error_required_field"|trans }}
            </span>
        </div>
    </div>
</div>

