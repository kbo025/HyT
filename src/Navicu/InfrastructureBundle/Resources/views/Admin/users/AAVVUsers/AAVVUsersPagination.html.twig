<div class="nvc-container">
    <div class="nvc-pagination nvc-pagination--admin">
        <div
            class="nvc-pagination--admin__previous"
            ng-class="{disabled: previousPageIsDisabled}"
            ng-click="!previousPageIsDisabled && getListFilter(search, pagination.previous, parameter, true)"
        >
            <i class="fa fa-angle-left"></i>
        </div>
        <div
            class="nvc-pagination--admin__next"
            ng-class="{disabled: nextPageIsDisabled}"
            ng-click="!nextPageIsDisabled && getListFilter(search, pagination.next, parameter, true)"
        >
            <i class="fa fa-angle-right"></i>
        </div>
    </div>
</div>
