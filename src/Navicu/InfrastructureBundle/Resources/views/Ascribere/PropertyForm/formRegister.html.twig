{% extends "NavicuInfrastructureBundle:Ascribere:layoutRegister.html.twig" %}

{% set header_title = {'module_title': '1'} %}

{% block body %}
<div
	ng-app="propertyRegisterModule"
	ng-controller="registerController as ctrl"
	novalidate
	ng-cloak>
	{% include "NavicuInfrastructureBundle:Ascribere:PropertyForm/topmenu.html.twig" with header_title %}
	<!--Top del formualario -->
	<div
		class="container"
		style="font-family:LatoRegular" >
		<div
			class="col-md-16"
			style="color:#572A85; font-size:18px;">
			<p align="justify">{{"extranet.formProperty.top_message"|trans }}</p>
		</div>
	</div>
	<br>
	<!--UbicaciÃ³n del establecimiento -->
	<form name="infoProperty">
		<div class="container form-font">
			<div class="jumbotron form-font-top form-group ">
				<div class="container form-font">
					<div class="col-sm-10 col-md-8 col-lg-6 right">
						<h3>{{"extranet.formProperty.location"|trans }}</h3>
					</div>
				</div>
				<br>
				<div class="container" style="color:#808080">
					<div class="row">
						<div class="col-md-4">
							<label
								ng-class="{'label-error' : infoProperty.name.$error.required &&
														   infoProperty.name.$touched}">
								{{"extranet.formProperty.name_property"|trans }}
								<strong>*</strong>
							</label>
							<input
								ng-class="{'input-error' : infoProperty.name.$error.required &&
														   infoProperty.name.$touched}"
								name="name"
								ng-model="form.name"
								style="width:100%"
								class="form-control my-imput"
								type="text"
							>
							<span
								class="error"
								ng-show="infoProperty.name.$error.required &&
										 infoProperty.name.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-3">
							<label
								ng-class="{'label-error' : infoProperty.accommodations.$error.required &&
														   infoProperty.accommodations.$touched}">
								{{"extranet.formProperty.type_of_accommodation"|trans }}
								<strong>*</strong>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.accommodations.$error.required &&
														   infoProperty.accommodations.$touched}"
								name="accommodations"
								ng-model="form.accommodation_name"
								style="width:100%"
								class="form-control my-imput"
								ng-options="id.name as id.name for (accommodation, id) in accommodationList | orderBy:'name'"
								ng-change="getId(searchId(accommodationList, form.accommodation_name), 'accommodation')">
							  	<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.accommodations.$error.required &&
										 infoProperty.accommodations.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-3">
							<label
								ng-class="{'label-error' : infoProperty.star.$error.required &&
														   infoProperty.star.$touched}">
								{{"extranet.formProperty.category_start"|trans }}
								<strong>*</strong>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.star.$error.required &&
														   infoProperty.star.$touched}"
								name="star"
								type="number"
								ng-model="form.star"
								class="form-control my-select"
								style="width:100%"
								ng-options="n for n in [] | range:1:5">
							  	<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.star.$error.required &&
										 infoProperty.star.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-3">
							<label
								ng-class="{'label-error' : infoProperty.phone.$error.required &&
														   infoProperty.phone.$touched ||
														   (infoProperty.phone.$error.pattern &&
														   infoProperty.phone.$touched)}">
								{{"extranet.formProperty.phone"|trans }}
								<strong>*</strong>
							</label>
							<input
								ng-class="{'input-error' : infoProperty.phone.$error.required &&
														   infoProperty.phone.$touched}"
								name="phone"
								ng-model="form.phones"
								style="width:100%"
								class="form-control my-imput"
								type="text"
								ng-pattern="/^\+?\d{2,3}-?\d{3}-?\d{6,7}/"
							>
							<span
								class="error"
								ng-show="infoProperty.phone.$error.required &&
										 infoProperty.phone.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<span
								class="error"
								ng-show="infoProperty.phone.$error.pattern &&
										 infoProperty.phone.$touched">
								{{ "share.validation.error_phone_lg"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-4 col-lg-3">
							<label
								ng-class="{'label-error' : infoProperty.fax.$error.pattern &&
														   infoProperty.fax.$touched}">
								{{"extranet.formProperty.fax_property"|trans }}
							</label>
							<input
								name="fax"
								ng-model="form.fax"
								style="width:100%"
								class="form-control my-imput"
								ng-pattern="/^\+?\d{2,3}-?\d{3}-?\d{6,7}/">
							<span
								class="error"
								ng-show="infoProperty.fax.$error.pattern &&
										 infoProperty.fax.$touched">
								{{ "share.validation.error_fax"|trans }}
							</span>
							<br>
						</div>
					</div>
					<br>
					<div class="row">
						<div class="col-md-4">
							<label
								ng-class="{'label-error' : infoProperty.country.$error.required &&
														   infoProperty.country.$touched}">
								{{"extranet.formProperty.country"|trans }}
								<strong>*</strong>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.country.$error.required &&
														   infoProperty.country.$touched}"
								name="country"
								class="form-control my-imput"
								style="width:100%"
								ng-model="pais"
								ng-options="val.name as val.name for (pais, val) in paises" >
								<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.country.$error.required &&
										 infoProperty.country.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-4">
							<label
								ng-class="{'label-error' : infoProperty.state.$error.required &&
														   infoProperty.state.$touched}">
								{{"extranet.formProperty.state"|trans }}
								<strong>*</strong>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.state.$error.required &&
														   infoProperty.state.$touched}"
								name="state"
								ng-disabled="!pais"
								class="form-control my-imput"
								style="width:100%"
								ng-model="estado"
								ng-options="valEstado.name as valEstado.name for (estado, valEstado) in states | orderBy:'name'" >
								<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.state.$error.required &&
										 infoProperty.state.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
						</div>
						<div class="col-md-4">
							<label
								ng-class="{'label-error' : infoProperty.city.$error.required &&
														   infoProperty.city.$touched}">
								{{"extranet.formProperty.town"|trans }}
								<strong>*</strong>
							</label>
							<select
								name="city"
								ng-class="{'input-error' : infoProperty.city.$error.required &&
														   infoProperty.city.$touched}"
								ng-disabled="!estado"
								class="form-control my-imput"
								style="width:100%"
								ng-model="municipio"
								ng-options="idCiudad.name as idCiudad.name for (ciudad, idCiudad) in towns | orderBy:'name'">
								<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.city.$error.required &&
										 infoProperty.city.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
						</div>
						<div class="col-md-4">
							<label
								ng-class="{'label-error' : infoProperty.city.$error.required &&
														   infoProperty.city.$touched}">
								{{"extranet.formProperty.parish"|trans }}
								<strong>*</strong>
							</label>
							<select
								name="city"
								ng-class="{'input-error' : infoProperty.city.$error.required &&
														   infoProperty.city.$touched}"
								ng-disabled="!municipio"
								class="form-control my-imput"
								style="width:100%"
								ng-model="parroquia"
								ng-options="idCiudad.name as idCiudad.name for (ciudad, idCiudad) in parishs | orderBy:'name'">
								<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.city.$error.required &&
										 infoProperty.city.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
						</div>
					</div>
					<div class="row">
						<div class="col-md-8">
							<label
								ng-class="{'label-error' : infoProperty.addres.$error.required &&
															  infoProperty.addres.$touched}">
								{{"extranet.formProperty.property_address"|trans }}
								<strong>*</strong>
							</label>
							<input
								ng-class="{'input-error' : infoProperty.addres.$error.required &&
														   infoProperty.addres.$touched}"
								name="addres"
								ng-model="dir"
								ng-disabled="!parroquia"
								style="width:100%"
								class="form-control my-imput"
								type="text">
							<span
								class="error"
								ng-show="infoProperty.addres.$error.required &&
										 infoProperty.addres.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
						</div>
					</div>
					<br>
					<div class="row">
                        <div class="col-md-16">
                            <label>
                            	{{"extranet.formProperty.location_of_property"|trans }}
                            	<strong>* {{"extranet.formProperty.marker_location_of_property"|trans }}</strong>
                            </label>
                            <div style="width:100%">
                                <div id="map_canvas">
                                    <ui-gmap-google-map
                                    	center="map.center"
                                    	zoom="map.zoom"
                                    	draggable="true"
                                    	options="options"
                                    	control="map.control"
                                    	ng-click="options.scrollwheel=true">
                                      	<ui-gmap-marker
                                      		coords="marker.coords"
                                      		options="marker.options"
                                      		events="marker.events"
                                      		idkey="marker.id"
                                      		control="marker.control">
                                      	</ui-gmap-marker>
                                    </ui-gmap-google-map>
                                </div>
                            </div>
                            <br>
                        </div>
					</div>
				</div>
			</div>
		</div>

		<!--Datos del estableciemiento-->
		<div class="container form-font">
			<div class="jumbotron form-font-top form-group">
				<div class="container">
					<div class="col-md-6 right">
						<h3>{{ "extranet.formProperty.property_details"|trans }}</h3>
					</div>
				</div>
				<br>
				<div class="container" style="color:#808080">
					<div class="row">
						<div class="col-md-3">
							<label
								ng-class="{'label-error' : infoProperty.opening_year.$error.required &&
														   infoProperty.opening_year.$touched}">
								{{ "extranet.formProperty.year_opened"|trans }}
								<strong>*</strong>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.opening_year.$error.required &&
														   infoProperty.opening_year.$touched}"
								name="opening_year"
								type="number"
								ng-options="n for n in [] | range2:2015:1899"
								ng-model="form.opening_year"
								class="form-control my-select"
								style="width:100%">
							  	<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.opening_year.$error.required &&
										 infoProperty.opening_year.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-3">
							<label>
								{{ "extranet.formProperty.last_renewal"|trans }}
								<strong>*</strong>
							</label>
							<select
								type="number"
								ng-options="n for n in [] | range2:2015:form.opening_year-1"
								ng-model="form.renewal_year"
								class="form-control my-select"
								style="width:100%">
							  	<option value="">Seleccione</option>
							</select>
							<br>
						</div>
						<div class="col-md-5">
							<label>
								{{ "extranet.formProperty.last_renovation"|trans }}
							</label>
							<select
								type="number"
								ng-model="form.public_areas_renewal_year"
								ng-options="n for n in [] | range2:2015:form.opening_year-1"
								class="form-control my-select"
								style="width:80%">
							    <option value="">Seleccione</option>
							</select>
							<br>
						</div>
						<div class="col-md-4" style=" margin-left: -6%;">
                            <label>{{ "extranet.formProperty.website"|trans }}</label>
                            <input
                            	placeholder="www.navicu.com"
                            	name="url"
                            	ng-model="form.url_web"
                            	style="width:100%"
                            	class="form-control my-imput"
                            	type="text"
                            >
						</div>
					</div>
					<div class="row">
						<div class="col-md-3">
							<label>{{ "extranet.formProperty.hotel_chain"|trans }}</label>
							<md-switch
								ng-model="form.hotel_chain"
								aria-label="Switch 2"
								class="md-warn">
								<span ng-if="form.hotel_chain == true">
									{{ "share.message.yes"|trans }}
								</span>
								<span ng-if="form.hotel_chain == false">
									{{ "share.message.no"|trans }}
								</span>
							</md-switch>
						</div>
						<div ng-show="form.hotel_chain">
							<div class="col-md-3">
								<div>
									<label>{{ "extranet.formProperty.name_chain"|trans }}</label>
									<input
										ng-model="form.hotel_chain_name"
										style="color:#000;"
										type="text"
										class="form-control">
								</div>
							</div>
						</div>
						<div class="col-md-3">
							<label
								class="tooltips tooltips-right"
								ng-class="{'label-error' : infoProperty.amount_room.$error.required &&
														   infoProperty.amount_room.$touched}">
								{{ "extranet.formProperty.rooms_number"|trans }}
								<i class="fa fn-info fa-x"></i>
								<strong>*</strong>
								<span style="width: 205% !important;margin-left: 15px !important;margin-top: -14%; !important">
									{{ "extranet.formProperty.message_rooms_amount"|trans }}
								</span>
							</label>
							<input
								ng-class="{'input-error' : infoProperty.amount_room.$error.required &&
														   infoProperty.amount_room.$touched}"
								name="amount_room"
								ng-model="form.amount_room"
								style="width:100%"
								class="form-control my-imput"
								type="number"
								min="1">
							<span
								class="error"
								ng-show="infoProperty.amount_room.$error.required &&
										 infoProperty.amount_room.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<span
								class="error"
								ng-show="infoProperty.amount_room.$error.number">
								{{ "share.validation.only_number"|trans }}
							</span>
							<span
								class="error"
								ng-show="infoProperty.amount_room.$error.min">
								{{ "share.validation.only_number_positive"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-3 col-lg-2">
							<label
								ng-class="{'label-error' : infoProperty.number_floor.$error.required &&
														   infoProperty.number_floor.$touched}">
								{{"extranet.formProperty.floors"|trans }}
								<strong>*</strong>
							</label>
							<select
								type="number"
								ng-class="{'input-error' : infoProperty.number_floor.$error.required &&
														   infoProperty.number_floor.$touched}"
								name="number_floor"
								ng-model="form.number_floor"
								class="form-control my-select"
								ng-options="n for n in [] | range:0:60">
							 	<option value="">Seleccione</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.number_floor.$error.required &&
										 infoProperty.number_floor.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-2">
							<label
								class="tooltips tooltips-right"
								ng-class="{'label-error' : infoProperty.check_in.$error.required &&
														   infoProperty.check_in.$touched}">
								{{"extranet.formProperty.check-in"|trans }}
								<i class="fa fn-info fa-x"></i>
								<strong>*</strong>
								<span style="width: 250% !important;margin-left: 0px !important;margin-top: -35%; !important">
									{{"extranet.formProperty.message_ckeckin"|trans }}
								</span>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.check_in.$error.required &&
													       infoProperty.check_in.$touched}"
								name="check_in"
								ng-model="form.check_in"
								class="form-control my-select"
								style="width:100%">
                                <option value="">Seleccione</option>
                                <option value="07:00:00">7:00</option>
                                <option value="08:00:00">8:00</option>
                                <option value="09:00:00">9:00</option>
                                <option value="10:00:00">10:00</option>
                                <option value="11:00:00">11:00</option>
                                <option value="12:00:00">12:00</option>
                                <option value="13:00:00">13:00</option>
                                <option value="14:00:00">14:00</option>
                                <option value="15:00:00">15:00</option>
                                <option value="16:00:00">16:00</option>
                                <option value="17:00:00">17:00</option>
                                <option value="18:00:00">18:00</option>
                                <option value="19:00:00">19:00</option>
                                <option value="20:00:00">20:00</option>
                                <option value="21:00:00">21:00</option>
                                <option value="22:00:00">22:00</option>
                                <option value="23:00:00">23:00</option>
                                <option value="00:00:00">00:00</option>
                                <option value="01:00:00">1:00</option>
                                <option value="02:00:00">2:00</option>
                                <option value="02:00:00">2:00</option>
                                <option value="03:00:00">3:00</option>
                                <option value="04:00:00">4:00</option>
                                <option value="05:00:00">5:00</option>
                                <option value="06:00:00">6:00</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.check_in.$error.required &&
										 infoProperty.check_in.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
							<br>
						</div>
						<div class="col-md-2">
							<label
								class="tooltips tooltips-right"
								ng-class="{'label-error' : infoProperty.check_out.$error.required &&
														   infoProperty.check_out.$touched}">
								{{ "extranet.formProperty.check-out"|trans }}
								<i class="fa fn-info fa-x"></i>
								<strong>*</strong>
								<span style="width: 230% !important;margin-left: -1px !important;margin-top: -30%; !important">
									{{ "extranet.formProperty.message_checkout"|trans }}
								</span>
							</label>
							<select
								ng-class="{'input-error' : infoProperty.check_out.$error.required &&
														   infoProperty.check_out.$touched}"
								name="check_out"
								ng-model="form.check_out"
								class="form-control my-select"
								style="width:100%">
                                <option value="">Seleccione</option>
                                <option value="07:00:00">7:00</option>
                                <option value="08:00:00">8:00</option>
                                <option value="09:00:00">9:00</option>
                                <option value="10:00:00">10:00</option>
                                <option value="11:00:00">11:00</option>
                                <option value="12:00:00">12:00</option>
                                <option value="13:00:00">13:00</option>
                                <option value="14:00:00">14:00</option>
                                <option value="15:00:00">15:00</option>
                                <option value="16:00:00">16:00</option>
                                <option value="17:00:00">17:00</option>
                                <option value="18:00:00">18:00</option>
                                <option value="19:00:00">19:00</option>
                                <option value="20:00:00">20:00</option>
                                <option value="21:00:00">21:00</option>
                                <option value="22:00:00">22:00</option>
                                <option value="23:00:00">23:00</option>
                                <option value="00:00:00">00:00</option>
                                <option value="01:00:00">1:00</option>
                                <option value="02:00:00">2:00</option>
                                <option value="02:00:00">2:00</option>
                                <option value="03:00:00">3:00</option>
                                <option value="04:00:00">4:00</option>
                                <option value="05:00:00">5:00</option>
                                <option value="06:00:00">6:00</option>
							</select>
							<span
								class="error"
								ng-show="infoProperty.check_out.$error.required &&
										 infoProperty.check_out.$touched">
								{{ "share.validation.input_required"|trans }}
							</span>
						</div>
					</div>
					<div class="row">
						<div
							class="container"
