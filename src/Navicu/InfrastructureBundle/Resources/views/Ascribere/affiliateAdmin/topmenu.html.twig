                        <a style="cursor:pointer;" ng-href="{{ path('navicu_admin_property_galleries', {'slug': slugTemp}) }}">
                            {% if module_title == "4"%}
                                <span class="fn-galeria" style="color:#FFF !important"></span>
                            {% else %}
                                <span class="fn-galeria color-icon-off"></span>
                            {% endif %}
                        </a>

                        <span style="color:#43254A">|</span> 

                        <!-- Registro de pago -->
                        
                        <a style="cursor:pointer;" ng-href="{{ path('navicu_admin_property_payment', {'slug': slugTemp}) }}">
                            {% if module_title == "5"%}
                                <span class="fn-pago"  style="color:#FFF !important"
                                ></span>
                            {% else %}
                                <span class="fn-pago color-icon-off"
                                ></span>
                            {% endif %}
                        </a> 

                        <span style="color:#43254A">|</span>
                       
                    </div>
