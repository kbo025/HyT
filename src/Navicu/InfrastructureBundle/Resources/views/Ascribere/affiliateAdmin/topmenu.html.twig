                        <!-- Información General -->
                        
                        <a style="cursor:pointer;" ng-href="{{ path('navicu_admin_property_general_information', {'slug': slugTemp}) }}">
                            {% if module_title == "1"%}
                                <span class="fn-informacion" style="color:#FFF !important"></span>
                            {% else %}
                                <span class="fn-informacion color-icon-off"></span>
                            {% endif %}
                        </a>
                        
                        <span style="color:#43254A">|</span>

                        <!-- Servicios -->
                        <a style="cursor:pointer;" ng-href="{{ path('admin_update_property_services', {'slug': slugTemp}) }}">
                            {% if module_title == "2"%}
                                <span class="fn-servicios" style="color:#FFF !important"></span>
                            {% else %}
                                <span class="fn-servicios color-icon-off"></span>
                            {% endif %}
                        </a>
                
                        <span style="color:#43254A">|</span>

