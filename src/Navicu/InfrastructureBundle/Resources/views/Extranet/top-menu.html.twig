{% block topmenu%}
    <nav class="navbar navbar-default navbar-default2" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#top-menu-extranet">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="collapse navbar-collapse" id="top-menu-extranet" style="margin-left:-15px;">
                <ul class="nav navbar-nav">
                    <li>
                        {% if module_title == "index"%}
                            <a style="color:#ffcf48;" href="{{ path('navicu_extranet_homepage', {'slug': slug }) }}">{{ "extranet.menu.home"|trans }}</a>
                        {% else %}
                            <a href="{{ path('navicu_extranet_homepage', {'slug': slug }) }}">{{ "extranet.menu.home"|trans }}</a>
                        {% endif %}
                    </li>
                    <li class="dropdown dropdown-hover">
                        {% if module_title == "inventory"%}
                            <a style="color:#ffcf48;" href="#" class="dropdown-toggle" data-toggle="dropdown">
                                {{ "extranet.menu.rates_and_inventory"|trans }} <span class="caret visible-xs-inline-block"></span>
                            </a>
                        {% elseif module_title == "load"%}
                            <a style="color:#ffcf48;" href="#" class="dropdown-toggle" data-toggle="dropdown">
                                {{ "extranet.menu.rates_and_inventory"|trans }} <span class="caret visible-xs-inline-block"></span>
                            </a>
                        {% else %}
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                {{ "extranet.menu.rates_and_inventory"|trans }} <span class="caret visible-xs-inline-block"></span>
                            </a>
                        {% endif %}
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                {% if slug is defined %}
                                    <a href="{{ path('Extranet_indexProperties', {'slug': slug, 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.room_inventory"|trans }}</a>
                                {% else %}
                                    <a href="#"> {{ "extranet.menu.room_inventory"|trans }}</a>
                                {% endif %}
                            </li>
                            <li>
                                {% if slug is defined %}
                                    <a href="{{ path('navicu_extranet_inventory_massload_get_data', {'slug': slug , 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.bulk_update"|trans }}</a>
                                {% else %}
                                    <a href="#"> {{ "extranet.menu.bulk_update"|trans }}</a>
                                {% endif %}
                            </li>
                            <li>
                                {% if is_granted('ROLE_ADMIN') %}
                                    <a href="{{ path('navicu_extranet_drop_inventory', {'slug': slug , 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.drop_inventory"|trans }}</a>
                                {% endif %}
                            </li>
                            <li class="divider visible-xs-block"></li>
                        </ul>
                    </li>
                    <li>
                        {% if module_title == "reservations" %}
                            <a style="color:#ffcf48;" href="{{ path('Extranet_reservations',  {'slug': slug , 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.reservations"|trans }}</a>
                        {% else %}
                            <a href="{{ path('Extranet_reservations', {'slug': slug , 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.reservations"|trans }}</a>
                        {% endif %}
                    </li>
                    {% if  not is_granted('ROLE_ADMIN') %}
                        <li>
                            {% if module_title == "tips" %}
                                <a style="color:#ffcf48;" href="{{ path('Extranet_tips') }}">{{ "extranet.menu.tips"|trans }}</a>
                            {% else %}
                                <a href="{{ path('Extranet_tips') }}">{{ "extranet.menu.tips"|trans }}</a>
                            {% endif %}
                        </li>
                    {% endif %}

                    {% if module_title == "historyOwner" %}
                        <li>
                            {% if slug is defined %}
                                <a style="color:#ffcf48;" href="{{ path('Extranet_logs', {'slug': slug, 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.historical"|trans }}</a>
                            {% else %}
                                <a href="#">{{ "extranet.menu.historical"|trans }}</a>
                            {% endif %}
                        </li>
                    {% elseif module_title == "historyMassload"%}
                        <li>
                        {% if slug is defined %}
                            <a style="color:#ffcf48;" href="{{ path('Extranet_logs', {'slug': slug, 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.historical"|trans }}</a></li>
                        {% else %}
                            <a href="#">{{ "extranet.menu.historical"|trans }}</a>
                        {% endif %}
                    {% elseif module_title == "historyInventory"%}
                        <li>
                            {% if slug is defined %}
                                <a style="color:#ffcf48;" href="{{ path('Extranet_logs', {'slug': slug, 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.historical"|trans }}</a>
                            {% else %}
                                <a href="#">{{ "extranet.menu.historical"|trans }}</a>
                            {% endif %}
                        </li>
                    {% else %}
                        <li>
                            {% if slug is defined %}
                                <a href="{{ path('Extranet_logs', {'slug': slug, 'extension' : app.request.attributes.get('extension')}) }}">{{ "extranet.menu.historical"|trans }}</a>
                            {% else %}
                                <a href="#">{{ "extranet.menu.historical"|trans }}</a>
                            {% endif %}
                        </li>
                    {% endif %}

                </ul>
            </div>
        </div>
    </nav>

    {% if module_title != "index" and module_title != "tips" %}
        <div id="background-historic">
            <div class="container">
                <div class="col-sm-12" style="color: #FFF;">
                    {% if module_title == "inventory" %}
                        <i class="fa fn-grilla fa-2x"></i> &nbsp;
                        <span class="top-menu-font2">
                            {{ "extranet.menu.room_inventory"|trans }} <small>{{ "share.word.for"|trans ~ " " ~ name }}</small>
                        </span>
                    {% elseif module_title == "load" %}
                        <i class="fa fn-cargamasiva fa-2x"></i> &nbsp;
                        <span class="top-menu-font2">
                            {{ "extranet.menu.bulk_update"|trans }} <small>{{ "share.word.for"|trans ~ " " ~ name }}</small>
                        </span>
                    {% elseif module_title == "historyOwner" %}
                        <i class="fa fn-historial fa-2x"></i> &nbsp;
                        <span class="top-menu-font2">
                            {{ "extranet.menu.historical"|trans }} <small>{{ "share.word.for"|trans ~ " " ~ name }}</small>
                        </span>
                    {% elseif module_title == "historyMassload" %}
                        <i class="fa fn-historial fa-2x"></i>
                        <span class="top-menu-font2">
                            {{ "extranet.menu.historical"|trans }} <small>{{ "share.word.for"|trans ~ " " ~ name }}</small>
                        </span>
                    {% elseif module_title == "historyInventory" %}
                        <i class="fa fn-historial fa-2x"></i>
                        <span class="top-menu-font2">
                            {{ "extranet.menu.historical"|trans }} <small>{{ "share.word.for"|trans ~ " " ~ name }}</small>
                        </span>
                    {% elseif module_title == "reservations" %}
                        <i class="fa fn-reserva fa-2x"></i>
                        <span class="top-menu-font2">
                            {{ "extranet.menu.reservations"|trans }} <small>{{ "share.word.for"|trans ~ " " ~ name }}</small>
                        </span>
                    {% endif %}
                </div>
            </div>
        </div>
    {% endif %}
{% endblock topmenu %} 