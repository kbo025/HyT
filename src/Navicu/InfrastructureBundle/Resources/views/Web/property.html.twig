        jQuery(function($) {
            $('#js-range-picker').rangePicker({
                checkInSelector: '.js-datepicker-check-in',
                checkOutSelector: '.js-datepicker-check-out'
            });

            $('#js-mobile-range-picker').rangePicker({
                checkInSelector: '.js-rangepicker-check-in',
                checkOutSelector: '.js-rangepicker-check-out'
            });

            $('#carousel').nvc-carousel({
                interval: 10000
            });
