{% extends "NavicuInfrastructureBundle:Web:layout-destiny.html.twig" %}

{% block seoMetadata %}
    {{parent()}}
    {% if app.environment == "prod" %}
		<title> {{ ("SEO.property." ~ app.request.attributes.get('slug') ~ ".title") | trans ({},'seo')}}</title>
        <meta property="og:title" content="{{ ("SEO.property." ~ app.request.attributes.get('slug') ~ ".title") | trans ({},'seo')}}"/>
        <meta property="og:description" content="{{ ("SEO.property." ~ app.request.attributes.get('slug') ~ ".description") | trans ({},'seo')}}"/>
        <meta property="og:image:secure_url" content="{{ 'https://navicu.com' ~ asset('images/home/carousel/reserva-de-alojamiento-online.jpg') }}"/>

        <meta name="title" content="{{ ("SEO.property." ~ app.request.attributes.get('slug') ~ ".title") | trans ({},'seo')}}">
        <meta name="description" content="{{ ("SEO.property." ~ app.request.attributes.get('slug') ~ ".description") | trans ({},'seo')}}">
		<link rel="canonical" href="{{ "https://www.navicu.com" ~ app.request.pathInfo() }}"/>
    {% endif %}
{% endblock  seoMetadata %}

{% block body %}
<!--- Breadcrumb -->
<section class="nvc-container hidden-xs hidden-sm" xmlns="http://www.w3.org/1999/html">
    <div class="col-xs-16">
        {#<ul class="nvc-breadcrumbs">
            <li> <a class="nvc-breadcrumbs--color" href="">Inicio</a></li>
            <li> <a class="nvc-breadcrumbs--color" href="">Portugal</a></li>
            <li> <a class="nvc-breadcrumbs--color" href="">Lisboa</a></li>
            <li> <a class="nvc-breadcrumbs--color" href="">Benfica</a></li>
            <li class="active nvc-breadcrumbs--color-current">Listado de la busqueda</li>
        </ul>#}
    </div>
</section>

<main class='nvc-container nvc-property' data-ng-app="propertyModule" data-ng-controller="propertyController">
    <div class="nvc-row">
        <div class="col-xs-16" >
            <div class="nvc-row">
                <div class="col-xs-16 hotel-margin ">
                    <!--- Hotel -->
                    <br>
                    <!--- Alerta -->
                    <div class="alert nvc-alert nvc-alert__info nvc-affix__alert hidden-xs">
                        <a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
                        <img src="{{ asset('images/preciogarantizadomin.png') }}" >
                        <p>Si encuentras una mejor oferta, <br>¡te hacemos un 5% de descuento!</p>
                    </div>
                    <section class="col-xs-16" data-ng-cloak>
                        <h2>{[{ data.name }]}</h2>
                        <p class="margin-star">
                            <span class="fa-stars fa fa-star" style="padding-left: 5px;" data-ng-repeat-n="data.star"></span>
                            <span class="font-numRoom" style="padding-left: 5px;">
                                {[{ data.amountRooms }]} {{ "share.register.room"|transchoice(2) }}</span>
                        </p>
                        <p> <span class="font-address">{[{ data.address }]}, {[{ data.municipality }]}, </span>
                            <span class=" font-address hidden-xs hidden-sm">{[{ data.state }]}, {[{ data.country }]}</span>
                        </p>
                        <p class="font-opening-year hidden-xs hidden-sm"> {{ "share.preview.opening_year"|trans }}: {[{ data.openingYear }]}
                           <span data-ng-if="data.renewalYear && data.renewalYear!=data.openingYear">, {{ "share.preview.last_reform"|trans }}: {[{ data.renewalYear }]}</span>
                        </p>
                        <div class="col-xs-16 hidden-xs" style="margin-top:-35px;" data-ng-if="data.information.designViewProperty==1">
                            <img
                                style="float: right; margin-top: -65px; width: 80px;"
                                src="{{ asset('images/preciogarantizadobig.png') }}"
                            >
                        </div>
                    </section>
                    <section
                        data-ng-if="data.information.designViewProperty==1"
                        class='col-xs-16 nvc-padding--xs post-body entry-content'
                        oncontextmenu='return false'
                        ondragstart='return false'
                        onmousedown='return false'
                        onselectstart='return false'>
                        <!-- Carrousel hotel (Imagen en grande)-->
                        <div
                            id="carousel"
                            class="carousel nvc-carousel nvc-carousel--property nvc-carousel__fade"
                            data-ride="carousel">
                            <div class="nvc-carousel__inner nvc-carousel__inner--property">
                                <div
                                    class="item"
                                    data-ng-class="{active: $first}"
                                    data-ng-repeat="image in data.images">
                                    {% verbatim %}
                                    <img data-ng-src="/uploads/images/images_original/{[{ image.url }]}" class="img-responsive">
                                    {% endverbatim %}
                                    <p align="left" class="banner-hotel">{[{ image.name }]}<p>
                                </div>
                            </div>
                        </div>
                        <!-- Carrousel pequeño hotel LG MD SM -->
                        <div class="clearfix hidden-xs">
                            <div
                                id="thumbcarousel"
                                class="carousel nvc-carousel nvc-carousel--property-thumbnail slide"
                                data-interval="false">
                                <div class="nvc-carousel__inner nvc-carousel__inner--property">
                                    <div class="item active">
                                        <div
                                            class="col-xs-4 col-sm-2 pad-carusel-hotel"
                                            data-target="#carousel"
                                            data-slide-to="{[{id}]}"
                                            data-ng-repeat="(id, favorite) in data.images">
                                            {% verbatim %}
                                            <img class="nvc-carousel__fill img-carusel-hotel"
                                                 data-ng-src="/uploads/images/images_sm/{[{favorite.url}]}">
                                            </img>
                                            {% endverbatim %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Carrousel pequeño hotel XS-->
                        <div
                            id="thumbcarousel2"
                            class="nvc-carousel nvc-carousel--property-thumbnail slide visible-xs"
                            data-interval="false">
                            <div class="nvc-carousel__inner nvc-carousel__inner--property" style="margin-top:-59px;">
                                <div class="item active">
                                    <div
                                        class="col-xs-4 pad-carusel-hotel-xs"
                                        data-target="#carousel"
                                        data-ng-repeat-n="4"
                                        data-slide-to="{[{ $index }]}">
                                        {% verbatim %}
                                        <img class="nvc-carousel__fill img-carusel-hotel"
                                            data-ng-src="/uploads/images/images_sm/{[{ data.images[$index].url}]}">
                                        </img>
                                        {% endverbatim %}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <section
                        data-ng-if="data.information.designViewProperty==2"
                        class='col-xs-16 col-sm-10 nvc-padding--xs post-body entry-content'
                        oncontextmenu='return false'
                        ondragstart='return false'
                        onmousedown='return false'
                        onselectstart='return false'>
                        <!-- Carrousel hotel (Imagen en grande)-->
                        <div
                            id="carousel"
                            class="carousel nvc-carousel nvc-carousel--property-min nvc-carousel__fade"
                            data-ride="carousel">
                            <div class="nvc-carousel__inner nvc-carousel__inner--property">
                                <div
                                    class="item"
                                    data-ng-class="{active: $first}"
                                    data-ng-repeat="image in data.images">
                                    {% verbatim %}
                                    <img data-ng-src="/uploads/images/images_original/{[{ image.url }]}" class="img-responsive">
                                    <p align="left" class="banner-hotel-min">{[{ image.name }]}<p>
                                    {% endverbatim %}
                                </div>
                            </div>
                        </div>
                        <!-- Carrousel pequeño hotel LG MD SM -->
                        <div class="clearfix hidden-xs">
                            <div
                                id="thumbcarousel"
                                class="nvc-carousel nvc-carousel--property-thumbnail slide"
                                data-interval="false">
                                <div class="nvc-carousel__inner nvc-carousel__inner--property">
                                    <div class="item active">
                                        <div
                                            class="col-xs-4 col-sm-2 pad-carusel-hotel"
                                            data-target="#carousel"
                                            data-slide-to="{[{id}]}"
                                            data-ng-repeat="(id, favorite) in data.images">
                                            {% verbatim %}
                                                <img class="nvc-carousel__fill img-carusel-hotel" data-ng-src="/uploads/images/images_sm/{[{ favorite.url }]}">
                                            {% endverbatim %}
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <!-- Carrousel pequeño hotel XS-->
                        <div
                            id="thumbcarousel2"
                            class="nvc-carousel nvc-carousel--property-thumbnail slide visible-xs"
                            data-interval="false">
                            <div class="nvc-carousel__inner nvc-carousel__inner--property" style="margin-top:-59px;">
                                <div class="item active">
                                    <div
                                        class="col-xs-4 pad-carusel-hotel-xs"
                                        data-target="#carousel"
                                        data-ng-repeat-n="4"
                                        data-slide-to="{[{ $index }]}">
                                        {% verbatim %}
                                        <img class="nvc-carousel__fill img-carusel-hotel" data-ng-src="/uploads/images/images_sm/{[{ data.images[$index].url }]}">
                                        {% endverbatim %}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    <!-- Servicios Destacados-->
                    <section
                        data-ng-if="data.information.designViewProperty==1"
                        class="col-xs-16 col-sm-10 col-md-8 nvc-padding nvc-row hidden-xs"
                        data-ng-cloak
                        style="height:215px; overflow-y:hidden; margin-bottom:25px; margin-left:10px; margin-top:20px;">
                        <h3 style="margin-top:0px;  margin-bottom:20px;"> {{ "share.preview.service_featured"|trans }}</h3>
                        <hr>
                        <div    class="col-sm-8"
                                data-ng-if="services.length>0 && service.service"
                                data-ng-repeat="service in services | orderBy: 'priority' ">
                            <p><i class="fa-services fa fn-{[{ service.icon }]}"></i>
                            <span class="font-services">{[{ service.service }]}</span></p>
                        </div>
                    </section>
                    <section
                        data-ng-if="data.information.designViewProperty==2"
                        class="col-xs-16 col-sm-6 nvc-padding nvc-row hidden-xs"
                        data-ng-cloak
                        style="height:220px; overflow-y:hidden; margin-bottom:5px; margin-left:10px;">
                        <h3 style="margin-top:0px;  margin-bottom:-15px;"> {{ "share.preview.service_featured"|trans }}</h3>
                        <hr>
                        <div    class="col-sm-8"
                                data-ng-if="services.length>0 && service.service"
                                data-ng-repeat="service in services | orderBy: 'priority' ">
                            <p><i class="fa-services fa fn-{[{ service.icon }]}"></i>
                            <span class="font-services">{[{ service.service }]}</span></p>
                        </div>
                    </section>
                    <!-- Mapa -->
                    <section
                        data-ng-if="data.information.designViewProperty==1"
                        class="col-xs-16 col-sm-6 col-md-8 nvc-row hidden-xs"
                        style="margin-top:20px;"
                        data-ng-cloak>
                        <div
                            width="auto"
                            height="auto"
                            frameborder="0"
                            style="border:5px solid #fff;  margin-left:40px;">
                            <div class="nvc-map">
                                <ui-gmap-google-map
                                    center="map.center"
                                    zoom="map.zoom"
                                    draggable="true"
                                    options="options"
                                    control="map.control"
                                    data-ng-click="options.scrollwheel=true">
                                    <ui-gmap-marker
                                        coords="marker.coords"
                                        options="marker.options"
                                        events="marker.events"
                                        idkey="marker.id"
                                        control="marker.control">
                                    </ui-gmap-marker>
                                </ui-gmap-google-map>
                            </div>
                        </div>
                    </section>
                    <section
                        data-ng-if="data.information.designViewProperty==2"
                        class="col-xs-16 col-sm-6 nvc-row hidden-xs"
                        style="margin-top:20px;"
                        data-ng-cloak>
                        <div
                            width="auto"
                            height="auto"
                            frameborder="0"
                            style="border:5px solid #fff;  margin-left:10px;">
                            <div class="nvc-map">
                                <ui-gmap-google-map
                                center="map.center"
                                zoom="map.zoom"
                                draggable="true"
                                options="options"
                                control="map.control"
                                data-ng-click="options.scrollwheel=true">
                                    <ui-gmap-marker
                                        coords="marker.coords"
                                        options="marker.options"
                                        events="marker.events"
                                        idkey="marker.id"
                                        control="marker.control">
                                    </ui-gmap-marker>
                                </ui-gmap-google-map>
                            </div>
                        </div>
                    </section>
                    <!--- Nav-Tab MD LG -->
                    <section class="col-xs-16 margin-nav-hotel hidden-xs hidden-sm" data-ng-cloak>
                        <div id="content">
                            <ul
                                id="tabs"
                                class="nvc-nav__pills nvc-nav__justified"
                                data-tabs="tabs">
                                <li class="active">
                                    <a href="#hab" data-toggle="tab">
                                        <i class="fa fn-rooms" style="padding-right:10px;"></i>
                                        {{ "word.room"|transchoice(2) }}
                                    </a>
                                </li>
                                <li>
                                    <a href="#service" data-toggle="tab">
                                        <i class="fa fn-services" style="padding-right:15px;"></i>
                                        {{ "word.service"|transchoice(2) }}
                                    </a>
                                </li>
                                <li>
                                    <a href="#inf" data-toggle="tab">
                                        <i class="fa fn-info"></i>
                                        {{ "message.important_information"|trans }}
                                    </a>
                                </li>
                                <li>
                                    <a href="#gal" data-toggle="tab">
                                        <i class="fa fn-galery"></i>
                                        {{ "word.gallery"|trans }}
                                    </a>
                                </li>
                            </ul>
                            <div id="my-tab-content" class="nvc-nav__content tab-content" data-ng-cloak>
                                <!--- Habitaciones -->
                                <div class="nvc-nav__pane tab-pane active" id="hab">
                                    {% if errorInventory %}
                                        <div class="nvc-row">
                                                <div class="alert alert-warning ">
                                                    <a href="#" class="close" data-dismiss="alert" aria-label="close" title="close">×</a>
                                                    <strong>
                                                        <i class="fa fn-info "></i> {{ "share.action.information"|trans }}!
                                                    </strong>
                                                    {{ "web.property.availability_has_changed"|trans }}
                                                </div>
                                        </div>
                                    {% endif %}
                                    <!--- Barra de Busqueda MD LG-->
                                    <div class="col-sm-16 nvc-padding nvc-affix__width">
                                        <div class="col-sm-16 nvc-nav__search nvc-form-inline">
                                            <div class="col-sm-4 select-date">
                                                <img class="search" src="{{ asset('images/lens.png') }}" >
                                                <strong>Selecciona tus fechas</strong>
                                            </div>
                                            <div class="nvc-form-group col-sm-12">
                                                <div class="nvc-search--destinations">
                                                    <div class="nvc-search__input-container">
                                                        <div class="nvc-search__select--children">
                                                            <i class="fa fn-child pull-left"></i>
                                                            <select
                                                                data-ng-model="search.kid"
                                                                tabindex="6"
                                                            >
                                                                <option ng-repeat="n in [] | range:0:8" value="{[{ n }]}">
                                                                    {[{ n }]} {[{ 'share.message.child' | translate:n }]}
                                                                </option>
                                                            </select>
                                                        </div>
                                                        <div class="nvc-search__select--adults">
                                                            <i class="fa fn-adult pull-left"></i>
                                                            <select
                                                                data-ng-model="search.adult"
                                                                tabindex="5"
                                                            >
                                                                <option ng-repeat="n in [] | range:1:9" value="{[{ n }]}">
                                                                    {[{ n }]} {[{ 'share.message.adult' | translate:n }]}
                                                                </option>
                                                            </select>
                                                        </div>
                                                        <div class="nvc-search__select--rooms">
                                                            <i class="fa fn-bedroom pull-left"></i>
                                                            <select
                                                                data-ng-model="search.room"
                                                                tabindex="4"
                                                            >
                                                                <option ng-repeat="n in [] | range:1:search.numTotalRooms" value="{[{ n }]}">
                                                                    {[{ n }]} {[{ 'share.message.room' | translate:n }]}
                                                                </option>
                                                            </select>
                                                        </div>
                                                        <div id="js-range-picker" style="margin-top: -31px;">
                                                            <div class="nvc-search__input--date-end">
                                                                <i class="fa fn-calendar pull-left"></i>
                                                                <input
                                                                    autocomplete="off"
                                                                    data-ng-model="search.endDate"
                                                                    name="startDate"
                                                                    placeholder="{[{ 'share.message.end_date' | translate }]}"
                                                                    tabindex="3"
                                                                    class="js-datepicker-check-out"
                                                                    type="text"
                                                                />
                                                            </div>
                                                            <div class="nvc-search__input--date-start">
                                                                <i class="fa fn-calendar pull-left"></i>
                                                                <input
                                                                    autocomplete="off"
                                                                    data-ng-model="search.startDate"
                                                                    name="startDate"
                                                                    placeholder="{[{ 'share.message.start_date' | translate }]}"
                                                                    tabindex="2"
                                                                    class="js-datepicker-check-in"
                                                                    type="text"
                                                                />
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!--- Tabla Habitaciones -->
                                    <div class="col-sm-16 nvc-padding">
                                        <div class="nvc-table__responsive" data-ng-if="flagRooms==true || isRequesting">
                                            <div class="loading-property-rooms-gif" data-ng-show="isRequesting">
                                                <img src="{{ asset('images/property-rooms-loading.gif') }}"/>
                                            </div>
                                            <form
                                                action="{{ path('navicu_reservation_confirmation') }}"
                                                method="post"
                                                name="form"
                                                data-ng-class="{ 'loading-property-rooms-overlay' : isRequesting }">
                                                <table class="nvc-table nvc-table__property">
                                                    <thead data-ng-if="roomProperty.length > 0">
                                                        <tr>
                                                            <th style="width: 26%;">{{ "share.preview.type_room"|trans }}</th>
                                                            <th style="width: 18%; line-height: 1.1;">{{ "share.preview.max"|trans }}</th>
                                                            <th style="width: 18%;">{{ "word.service"|transchoice (1) }}</th>
                                                            <th style="width: 10%;">{{ "share.preview.num_room"|trans }}</th>
                                                            <th style="width: 12%;">{{ "share.preview.price"|trans }}</th>
                                                            <th >{{ "share.preview.reservation"|trans }}</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <input type="hidden" name="checkIn" value="{[{search.startDate}]}"/>
                                                        <input type="hidden" name="checkOut" value="{[{search.endDate}]}"/>
                                                        <input type="hidden" name="slug" value="{[{search.slug}]}"/>
                                                        <input type="hidden" name="numberAdult" value="{[{search.adult}]}"/>
                                                        <input type="hidden" name="numberChild" value="{[{search.kid}]}"/>
                                                        <input type="hidden" name="countryCode" value="{[{data.countryCode}]}"/>
                                                        <input type="hidden" name="type" value="{[{data.type}]}"/>
                                                        <tr
                                                            data-ng-repeat="(id, room) in roomProperty"
                                                            data-ng-init="indexRoom = id">
                                                            <!--- Columna Habitación -->
                                                            <input
                                                                data-ng-if="room.amountPackagesSelected>0"
                                                                type="hidden"
                                                                value="{[{room.idRoom}]}"
                                                                name="rooms[{[{ room.pos }]}][idRoom]"/>
                                                            <input
                                                                data-ng-if="room.amountPackagesSelected>0"
                                                                type="hidden"
                                                                value="{[{temporal[indexRoom].bed}]}"
                                                                name="rooms[{[{ room.pos }]}][idBedsRooms]"/>
                                                            <!--- Tipo de habitación -->
                                                            <td
                                                                data-ng-class="{ 'td-border' : id===roomProperty.length-1}">
                                                                <div
                                                                    class="table-p-pad img-table-pad"
                                                                    data-ng-click="showRoomModal(room)">
                                                                    <div class="room-zoom">
                                                                        <a href="" >
                                                                            <i class="fa-plus-img fa fa-plus-circle"></i>
                                                                        </a>
                                                                    </div>
                                                                    {% verbatim %}
                                                                        <img
                                                                            class="nvc-carousel__fill img-table"
                                                                            data-ng-src="/uploads/images/images_sm/{[{room.profileImage.url}]}">
                                                                    {% endverbatim %}
                                                                </div>
                                                                <h4 class="table-p">{[{ room.name }]} </h4>
                                                                <select
                                                                    class="nvc-select--property"
                                                                    style="width:60%; color:#A81F6E; margin-bottom:10px;"
                                                                    data-ng-model="temporal[indexRoom].bed"
                                                                    data-ng-options="val.id as val.name for (bedselected, val) in room.bedsRooms"
                                                                    data-ng-change="optionDelete()"
                                                                >
                                                                    <option
                                                                        ng-repeat="val.id as val.name for (bedselected, val) in room.bedsRooms"
                                                                        ng-value="val.id"
                                                                        ng-selected="temporal[indexRoom].bed === bedselected)"
                                                                    >
                                                                        {[{ val }]}
                                                                    </option>
                                                                </select>
                                                                <p
                                                                    class="table-p font-condition"
                                                                    data-ng-if="data.information.taxRate > 0"
                                                                >
                                                                    {{ "share.preview.include"|trans }}: {[{ data.information.taxRate }]}%
                                                                    {{ "share.preview.tax"|trans }}
                                                                </p>
                                                                <p class="table-p font-condition">
                                                                    <span>Política de cancelación no reembolsable</span>
                                                                </p>
                                                            </td>
                                                            <!--- Nº de personas -->
                                                            <td
                                                                data-ng-class="{ 'td-border' : id===roomProperty.length-1}"
                                                                class="tab-center table-align"
                                                                style="padding-top:90px;"
                                                            >
                                                                <div class="col-md-8">
                                                                    <label><i class="fa fn-user"></i> Adultos</label>
                                                                    <select
                                                                        class="nvc-select--property"
                                                                        style="display:inherit; width:50px;"
                                                                        data-ng-model="room.packages[room.idPack].numPerson"
                                                                        data-ng-options="n for n in [] | range:room.minPeople:room.maxPeople"
                                                                        data-ng-change="changeNumberPeople(room.packages[room.idPack],room.maxPeople, data.information.conditionAge.child, room.rules)"
                                                                    >
                                                                    </select>
                                                                </div>
                                                                <div class="col-md-8">
                                                                    <label data-ng-if="data.information.conditionAge.child==true">
                                                                         <i class="fa fn-info info nvc-tooltips nvc-tooltips__top"
                                                                            data-ng-if="data.information.conditionAge.agePolicy.length!=0"
                                                                        >
                                                                            <span style="margin-left:-38px; width:190px;">
                                                                                Niños entre los {[{data.information.conditionAge.agePolicy.child[0]}]} y los {[{data.information.conditionAge.agePolicy.child[1]}]} años
                                                                            </span>
                                                                        </i>
                                                                        <i class="fa fn-user child"></i>
                                                                        Niños
                                                                    </label>
                                                                    <select
                                                                        data-ng-if="data.information.conditionAge.child==true"

                                                                        class="nvc-select--property"
                                                                        style="display:inherit;  width:50px;"
                                                                        data-ng-model="room.packages[room.idPack].numberChild"
                                                                        data-ng-options="n for n in [] | range:0:room.maxPeople-room.packages[room.idPack].numPerson"
                                                                        data-ng-change="asignPrice(room.packages[room.idPack], data.information.conditionAge.child, room.rules)"
                                                                    >
                                                                    </select>
                                                                </div>
                                                                <div
                                                                    class="col-md-8"
                                                                    ng-repeat="n in getRanges(room.packages[room.idPack].numberChild) track by $index"
                                                                >
                                                                    <label data-ng-if="data.information.conditionAge.child==true"
                                                                    >
                                                                        <i class="fa fn-user child"></i>
                                                                        Edad Niño {[{ $index + 1 }]}
                                                                    </label>
                                                                    <select
                                                                            data-ng-if="data.information.conditionAge.child==true"
                                                                            class="nvc-select--property"
