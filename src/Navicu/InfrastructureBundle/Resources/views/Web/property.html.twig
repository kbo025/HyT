                offset: {
                    top: 80,
                    bottom: function () {
                        return (this.bottom = $('.footer').outerHeight(true))
                    }
                }
            });
        });

        //**** Función para mover la barra lateral cuando se haga scroll en la tabla de habitaciones
        function updateSidebar() {
            // Padding del elemento superior.
            var padding = 8;

            // Márgenes que queremos que tenga el elemento "móvil".
            var marginTop = -10;
            var marginBottom = 0;

            // Elementos.
            var element = document.querySelector('.nav-reserve');
            if (!element) {
                return false;
            }

            var parent = element.parentElement;

            // Límites.
            var elementBounds = element.getBoundingClientRect();
            var parentBounds = parent.getBoundingClientRect();

            // Alturas.
            var elementHeight = elementBounds.height;
            var parentHeight = parentBounds.height;

            // No podemos mover un elemento con una altura mayor o igual a la de
            // su elemento superior.
            if (elementHeight >= parentHeight) {
                return;
            }

            // La distancia que tendrá será un 25% de la altura de la ventana.
            var distance = window.innerHeight * 0.25;

            var y = window.scrollY;

            // `a` es la posición absoluta del borde superior del elemento superior.
            // `b` es la posición absoluta del borde inferior del elemento superior.
            var a = y + parentBounds.top;
            var b = y + parentBounds.bottom;

            // Calcula la posición del affix.
            var top = y-a + distance;

            // Si la posición calculada se pasa de los límites, sustituirla
            // por su límite más cercano.
            //
            // Por ejemplo, si calculamos que está más abajo de lo que debería,
            // la ponemos lo más abajo posible, sin pasarnos del límite.
            if (top < marginTop) {
                top = marginTop;
            } else if (top + elementHeight >= parentHeight-(padding*2) - marginBottom) {
                top = parentHeight - elementHeight-(padding*2) - marginBottom;
            }

            // Aplicamos el estilo.
            element.style.top = '' + top + 'px';

            return true;
        }
        $(window).resize(updateSidebar);
        $(window).scroll(updateSidebar);
       	//****
    </script>
